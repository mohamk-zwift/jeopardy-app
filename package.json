  "description": "NOTE: as of Oct. 10, 2020, Webpack has released verion 5.0. I do not know how to use this yet, therefore this setup is meant to setup webpack version 4 (`npm install webpack@4.43.0 webpack-cli@3.3.11 --save-dev`)",
  "main": "webpack.common.js",
  "scripts": {
    "dev": "webpack --config webpack.dev.js",
    "dev:server": "webpack --config webpack.dev.js && webpack-dev-server --open --config webpack.dev.js",
    "prod": "webpack --config webpack.prod.js",
    "watch": "webpack --watch --config webpack.dev.js",
    "server": "webpack-dev-server --open --config webpack.dev.js",
    "format": "prettier --ignore-path .gitignore --write .",
    "lint": "eslint --ignore-path .gitignore .",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [],
